#version 330 core
out vec4 FragColor;

//in vec4 ourColor;
in vec2 TexCoord;

uniform sampler2D texture2D;
//uniform sampler2D alphaMask;

void main()
{
//vec4 alpha=texture(alphaMask, TexCoord);
vec4 color=texture(texture2D, TexCoord);
//FragColor = vec4(color.x,color.y,color.z,alpha.x);
FragColor =color;
};